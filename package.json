{
  "name": "reddesk",
  "version": "0.0.5",
  "description": "Reddit for Desktop",
  "main": "electron.js",
  "scripts": {
    "test": "tsc && electron .",
    "test-windows": "@powershell -NoProfile -ExecutionPolicy Unrestricted -Command tsc; electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder --macos --linux --windows",
    "full_dist": "electron-builder --dir && electron-builder --macos --linux --windows",
    "clean": "rm -rf dist/*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GlennCottam/RedDesk.git"
  },
  "keywords": [
    "Reddit",
    "Desktop",
    "WebApp",
    "Electron"
  ],
  "author": "Glenn Cottam",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/GlennCottam/RedDesk/issues"
  },
  "homepage": "https://github.com/GlennCottam/RedDesk#readme",
  "devDependencies": {
    "angular": "^1.7.8",
    "electron": "^5.0.5",
    "electron-builder": "^20.44.4",
    "tsconfig": "7.0.0",
    "typescript": "^3.5.2"
  }
}
